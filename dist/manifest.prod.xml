<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<!-- 
  PRODUCTION MANIFEST
  Replace tankuday21 and GeminiForExcel with your actual values
  Example: https://yourusername.github.io/excel-ai-copilot/
-->
<OfficeApp xmlns="http://schemas.microsoft.com/office/appforoffice/1.1" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:bt="http://schemas.microsoft.com/office/officeappbasictypes/1.0" xmlns:ov="http://schemas.microsoft.com/office/taskpaneappversionoverrides" xsi:type="TaskPaneApp">
  <Id>1f252c93-dff7-4104-a669-c1e2faf64a90</Id>
  <Version>3.1.1.0</Version>
  <ProviderName>Your Name</ProviderName>
  <DefaultLocale>en-US</DefaultLocale>
  <DisplayName DefaultValue="Excel AI Copilot"/>
  <Description DefaultValue="AI-powered assistant for Excel - analyze data, create formulas, generate charts, and automate tasks."/>
  <IconUrl DefaultValue="https://tankuday21.github.io/GeminiForExcel/assets/icon-32.png"/>
  <HighResolutionIconUrl DefaultValue="https://tankuday21.github.io/GeminiForExcel/assets/icon-64.png"/>
  <SupportUrl DefaultValue="https://github.com/tankuday21/GeminiForExcel"/>
  <AppDomains>
    <AppDomain>https://tankuday21.github.io</AppDomain>
  </AppDomains>
  <Hosts>
    <Host Name="Workbook"/>
  </Hosts>
  <DefaultSettings>
    <SourceLocation DefaultValue="https://tankuday21.github.io/GeminiForExcel/taskpane.html"/>
  </DefaultSettings>
  <Permissions>ReadWriteDocument</Permissions>
  <VersionOverrides xmlns="http://schemas.microsoft.com/office/taskpaneappversionoverrides" xsi:type="VersionOverridesV1_0">
    <Hosts>
      <Host xsi:type="Workbook">
        <DesktopFormFactor>
          <GetStarted>
            <Title resid="GetStarted.Title"/>
            <Description resid="GetStarted.Description"/>
            <LearnMoreUrl resid="GetStarted.LearnMoreUrl"/>
          </GetStarted>
          <FunctionFile resid="Commands.Url"/>
          <ExtensionPoint xsi:type="PrimaryCommandSurface">
            <OfficeTab id="TabHome">
              <Group id="CommandsGroup">
                <Label resid="CommandsGroup.Label"/>
                <Icon>
                  <bt:Image size="16" resid="Icon.16x16"/>
                  <bt:Image size="32" resid="Icon.32x32"/>
                  <bt:Image size="80" resid="Icon.80x80"/>
                </Icon>
                <Control xsi:type="Button" id="TaskpaneButton">
                  <Label resid="TaskpaneButton.Label"/>
                  <Supertip>
                    <Title resid="TaskpaneButton.Label"/>
                    <Description resid="TaskpaneButton.Tooltip"/>
                  </Supertip>
                  <Icon>
                    <bt:Image size="16" resid="Icon.16x16"/>
                    <bt:Image size="32" resid="Icon.32x32"/>
                    <bt:Image size="80" resid="Icon.80x80"/>
                  </Icon>
                  <Action xsi:type="ShowTaskpane">
                    <TaskpaneId>ButtonId1</TaskpaneId>
                    <SourceLocation resid="Taskpane.Url"/>
                  </Action>
                </Control>
              </Group>
            </OfficeTab>
          </ExtensionPoint>
        </DesktopFormFactor>
      </Host>
    </Hosts>
    <Resources>
      <bt:Images>
        <bt:Image id="Icon.16x16" DefaultValue="https://tankuday21.github.io/GeminiForExcel/assets/icon-16.png"/>
        <bt:Image id="Icon.32x32" DefaultValue="https://tankuday21.github.io/GeminiForExcel/assets/icon-32.png"/>
        <bt:Image id="Icon.80x80" DefaultValue="https://tankuday21.github.io/GeminiForExcel/assets/icon-80.png"/>
      </bt:Images>
      <bt:Urls>
        <bt:Url id="GetStarted.LearnMoreUrl" DefaultValue="https://github.com/tankuday21/GeminiForExcel"/>
        <bt:Url id="Commands.Url" DefaultValue="https://tankuday21.github.io/GeminiForExcel/commands.html"/>
        <bt:Url id="Taskpane.Url" DefaultValue="https://tankuday21.github.io/GeminiForExcel/taskpane.html"/>
      </bt:Urls>
      <bt:ShortStrings>
        <bt:String id="GetStarted.Title" DefaultValue="Excel AI Copilot is ready!"/>
        <bt:String id="CommandsGroup.Label" DefaultValue="AI Copilot"/>
        <bt:String id="TaskpaneButton.Label" DefaultValue="AI Copilot"/>
      </bt:ShortStrings>
      <bt:LongStrings>
        <bt:String id="GetStarted.Description" DefaultValue="Your AI assistant is ready. Click 'AI Copilot' in the Home tab to start analyzing data, creating formulas, and more."/>
        <bt:String id="TaskpaneButton.Tooltip" DefaultValue="Open AI Copilot to get intelligent assistance with your Excel data"/>
      </bt:LongStrings>
    </Resources>
  </VersionOverrides>
</OfficeApp>
